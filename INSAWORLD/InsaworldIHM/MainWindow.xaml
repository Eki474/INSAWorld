        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="New Game" HorizontalAlignment="Left" Margin="211,163,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button_Copy" Content="Load" HorizontalAlignment="Left" Margin="211,199,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button_Copy1" Content="Replay" HorizontalAlignment="Left" Margin="211,235,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.606,3.699"/>
