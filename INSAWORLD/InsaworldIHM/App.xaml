<Application x:Class="InsaworldIHM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:InsaworldIHM"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <Style x:Key="Square">
            <Setter Property="TextElement.FontFamily" Value="Ressources/fonts/#Squares Bold"/>
        </Style>
        <Style x:Key="Blank">
            <Setter Property="TextElement.FontFamily" Value="Ressources/fonts/#blank"/>
        </Style>
    </Application.Resources>
</Application>
